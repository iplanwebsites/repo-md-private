{
  "name": "@repo-md/processor",
  "type": "module",
  "version": "0.0.3",
  "scripts": {
    "ppp": "npm version patch && git push && npm publish",
    "build": "tsup",
    "postbuild": "cp -r dist package.json ../repo-build-worker-legacy/src/modules/repo-processor/",
    "dev": "npm run build -- --watch",
    "format": "prettier --write src",
    "test": "vitest --run",
    "convert": "node command/cli.js",
    "convert:example": "node command/cli.js -i ./example-vault -o output.json",
    "convert:dev": "node command/cli.js --debug 2",
    "kill-port": "lsof -ti:3456 | xargs kill -9 2>/dev/null || true",
    "pf": "npm run kill-port && npm run build && node test/portfolio-test.js",
    "preview": " node test/previewer/previewer-server.js",
    "previewpf": " nodemon test/previewer/previewer-server.js ./test/portfolio-dist & wait-on http://localhost:3456 && open http://localhost:3456"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/repo-md/repo-processor.git"
  },
  "private": true,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@portaljs/remark-wiki-link": "^1.2.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@wordpress/wordcount": "^4.22.0",
    "commander": "^13.1.0",
    "gray-matter": "^4.0.3",
    "hast-util-from-html": "^2.0.2",
    "hast-util-heading": "^3.0.0",
    "hast-util-to-text": "^4.0.2",
    "i": "^0.3.7",
    "ignore": "^5.3.2",
    "mdast-util-to-string": "^4.0.0",
    "npm": "^11.2.0",
    "ora": "^8.2.0",
    "playwright": "^1.53.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-mathjax": "^6.0.0",
    "rehype-mermaid": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.1",
    "remark-callouts": "^2.0.0",
    "remark-gfm": "^4.0.0",
    "remark-images": "^4.1.0",
    "remark-math": "^6.0.0",
    "remark-obsidian-link": "^0.2.4",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-wiki-link-plus": "^1.1.1",
    "remark-youtube": "github:iplanwebsites/remark-youtube#master",
    "sharp": "^0.33.5",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.5.5",
    "@types/underscore": "^1.11.15",
    "express": "^4.18.2",
    "hastscript": "^9.0.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "unist-util-inspect": "^8.1.0",
    "vite": "^5.4.5",
    "vitest": "^2.1.1",
    "wait-on": "^8.0.3"
  }
}
