    "push:vintage": "git push dokku main:master",
    "push": "git push dokku main:main",
    "airtable": "node scripts/refreshAirtable.js",
    "at": "npm run airtable",
    "commit-changes": "git add . && git commit -m \"ðŸ”„ blog and airtable refresh\" || true",
