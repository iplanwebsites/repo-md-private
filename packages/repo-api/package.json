    "b": "run-s obsidian:build-parser obsidian:convert",
    "update": "run-s blog airtable commit-changes push",
    "cloc": "find src app lib -type f | grep -v 'components/ui' | grep -v 'node_modules' | grep -v 'dist' | grep -v 'build' | grep -v 'components_DEV' | grep -v '.git' | grep -v '.cache' | grep -v 'coverage' | xargs npx cloc --by-file-by-lang",
    "instructor": "node scripts/test-instructor.js",
    "t": "PORT=${PORT:-5599} && npx ngrok http $PORT && open-cli http://localhost:4040",
    "license": "license-checker-rseidelsohn --markdown --out license.md ",
    "up": "npm i repo-md@latest",
    "test:weather": "node scripts/testWeatherTool.js",
    "demo:weather": "node scripts/testWeatherDemo.js",
    "test:chat-tools": "node scripts/testChatTools.js",
    "test:chat-file-tools": "node scripts/testChatFileTools.js",
    "test:nl": "node scripts/testEditorChatNL.js",
    "test:project-context": "node scripts/testProjectContext.js",
    "test:tools": "node scripts/testTools.js",
    "kill": "lsof -ti:5599 | xargs kill -9"
