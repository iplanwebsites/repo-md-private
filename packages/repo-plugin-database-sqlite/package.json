    "@types/better-sqlite3": "^7.6.11"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "repo-md",
    "plugin",
    "database",
    "sqlite",
    "vector-search"
