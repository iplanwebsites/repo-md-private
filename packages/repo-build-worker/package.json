{
  "name": "@repo-md/worker",
  "version": "1.0.0",
  "type": "module",
  "main": "src/worker.js",
  "scripts": {
    "start": "node src/worker.js",
    "dev": "nodemon src/worker.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:spec": "node scripts/testSpecFiles.js",
    "test:raccook": "node scripts/testRaccook.js",
    "test:wp": "node scripts/test-wp-importer.js",
    "test:img": "node scripts/testImageEmbeddings.js",
    "test:docker": "node scripts/test-dockerization.js",
    "test:docker-build": "node scripts/test-docker-build.js",
    "test:dokku": "node scripts/test-deploy-locally.js",
    "test:optimization": "node scripts/testUploadOptimization.js",
    "test:media-hash": "node scripts/testMediaHashOptimization.js",
    "test:frontmatter": "node scripts/testFrontmatterSchema.js",
    "test:frontmatter-sqlite": "node scripts/testFrontmatterSQLite.js",
    "testProjectStarterGenerator": "node scripts/test-project-starter.js",
    "testFullWorkflow": "node scripts/test-full-workflow.js",
    "copy:repo-processor": "node scripts/copy-repo-processor.js",
    "wordpress-export-to-markdown ": "npm run wordpress-export-to-markdown ",
    "update-parser": "cd ../repo-processor && npm run build && cd ../repo-worker && npm uninstall obsidian-parser-dev && npm install",
    "dokku": "git push dokku main:master",
    "kill": "lsof -ti:5522 | xargs kill -9",
    "cf:deploy": "bash scripts/cf-deploy.sh",
    "cf:deploy:dev": "bash scripts/cf-deploy.sh dev",
    "cf:deploy:staging": "bash scripts/cf-deploy.sh staging",
    "cf:deploy:production": "bash scripts/cf-deploy.sh production",
    "cf:test": "bash scripts/cf-test.sh",
    "cf:dev": "bash scripts/cf-local-dev.sh",
    "cf:dev:docker": "USE_DOCKER=true bash scripts/cf-local-dev.sh",
    "cf:manage": "node scripts/cf-manage.js",
    "cf:status": "node scripts/cf-manage.js status",
    "cf:logs": "node scripts/cf-manage.js logs",
    "cf:build": "docker build -f Dockerfile.cf -t repo-build-worker:local .",
    "cf:cleanup": "node scripts/cf-manage.js cleanup",
    "cf:workflow": "node tests/test-cf-workflow.js",
    "cf:workflow:build": "node tests/test-cf-workflow.js --build",
    "cf:login": "npx wrangler login"
  },
  "keywords": [
    "worker",
    "gcp",
    "cloud-run"
  ],
  "author": "",
  "private": true,
  "description": "Repository worker service for processing jobs in GCP Cloud Run",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.24",
    "@aws-sdk/client-s3": "^3.802.0",
    "@aws-sdk/s3-request-presigner": "^3.802.0",
    "@huggingface/transformers": "^3.5.0",
    "ask-secret": "^1.0.2",
    "better-sqlite3": "^12.4.1",
    "compute-cosine-similarity": "^1.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "glob": "^10.4.5",
    "graphology": "^0.26.0",
    "ignore": "^7.0.5",
    "obsidian-parser-prod-NOTNEEDED": "github:iplanwebsites/obsidian-parser",
    "octokit": "^4.1.3",
    "openai": "^5.8.2",
    "rehype-mermaid": "^3.0.0",
    "@repo-md/processor": "file:../repo-processor",
    "sqlite-vec": "^0.1.7-alpha.2",
    "vectra": "^0.11.0",
    "wink-bm25-text-search": "^3.1.2",
    "wordpress-export-to-markdown": "^3.0.4",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "miniflare": "^4.20250428.0",
    "nodemon": "^3.1.0",
    "wrangler": "^4.45.3"
  },
  "engines": {
    "node": ">=18"
  }
}
