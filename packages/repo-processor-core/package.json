{
  "name": "@repo-md/processor-core",
  "version": "0.1.0",
  "description": "Core markdown processor for repo.md - converts Obsidian vaults to structured JSON",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:parity": "vitest run --grep parity",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "gray-matter": "^4.0.3",
    "hast-util-to-string": "^3.0.1",
    "mdast-util-to-string": "^4.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/hast": "3.0.4",
    "@types/mdast": "4.0.4",
    "@types/node": "^22.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "markdown",
    "obsidian",
    "processor",
    "cms",
    "headless-cms"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iplanwebsites/repo-md"
  }
}
